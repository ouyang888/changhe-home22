page {
  background: #F9F9F9;
}

.my-bg {
  background: #F36433;
  height: 144rpx;
  width: 100%;
}

.my-top-box {
  background: #fff;
  width: 90%;
  margin: 0 auto;
  border-radius: 20rpx;
  box-shadow: 0px 0px 16rpx 0px rgba(0, 0, 0, 0.08);
  position: absolute;
  top: -120rpx;
  left: 0;
  right: 0;
  z-index: 10;
}

.top-flex {
  display: flex;
  justify-content: space-between;
}

.user-img {
  margin-left: 34rpx;
  margin-top: 62rpx;
}

.user-img image {
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
}

.user-name {
  margin-left: 34rpx;
}

.user-name view {
  font-size: 24rpx;
  color: #808080;
  margin-top: 36rpx;
}

.user-name view:last-child {
  padding-bottom: 36rpx;
}

.user {
  font-size: 30rpx !important;
  color: #1A1A1A !important;
}

.bing-wechat {
  margin-top: 40rpx;
  margin-right: 68rpx;
}

.bing-wechat button {
  background: #F36433;
  font-size: 28rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx !important;
  height: 30rpx !important;
  font-weight: 400;
  white-space: nowrap;
  border-radius: 0%;
  padding: 26rpx;
}

.my-middle-bg {
  background: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  position: absolute;
  top: -40rpx;
  z-index: 5;
  left: 0;
  right: 0;
  padding-top: 228rpx;
}

.my-menu-flex {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 90%;
  margin: 0 auto;
  padding-bottom: 80rpx;
}

.my-menu-flex:last-child {
  padding-bottom: 60rpx;
}

.menu-img {
  color: #333333;
  font-size: 28rpx;
  display: flex;
  align-items: center;
}

.menu-img image {
  width: 42rpx;
  height: 40rpx;
}

.right-img {
  width: 14rpx;
  height: 26rpx;
}

.right-img image {
  width: 100%;
  height: 100%;
}

.menu-psd-img image {
  width: 40rpx !important;
  height: 44rpx !important;
}