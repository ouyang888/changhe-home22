page {
  background: #F9F9F9;
}
.add-top-box {
  width: 94%;
  background: #fff;
  border-radius: 20rpx;
  margin: 21rpx auto;
}
.picker {
  color: #333333;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-top: 57rpx;
}

.picker text {
  color: #666666;
  font-size: 28rpx;
  padding-right: 50rpx;
  padding-left: 38rpx;
}

.picker-img {
  width: 14rpx;
  height: 24rpx;
  display: flex;
  align-items: center;
  padding-right: 35rpx;
}

.picker-img image {
  width: 100%;
  height: 100%;
}